{
  "name": "service-boilerplate",
  "version": "1.0.0",
  "description": "A barebones nodejs repository with all the standard stuffs",
  "main": "index.js",
  "scripts": {
    "build": "gulp",
    "dev": "gulp dev",
    "lint": "gulp lint",
    "start": "babel-node $npm_package_main",
    "test": "gulp",
    "test:watch": "ava **/*.spec.js -wv"
  },
  "engines": {
    "node": ">=6"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n8io/service-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "service",
    "api"
  ],
  "author": "Nate Clark <n8@n8io.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n8io/service-boilerplate/issues"
  },
  "homepage": "https://github.com/n8io/service-boilerplate#readme",
  "dependencies": {
    "body-parser": "^1",
    "cwd": "^0",
    "dotenv-safe": "^2",
    "express": "^4",
    "lodash": "^4"
  },
  "devDependencies": {
    "ava": "^0",
    "babel-cli": "^6",
    "babel-core": "^6",
    "babel-eslint": "^4",
    "babel-plugin-transform-runtime": "^6",
    "babel-preset-es2015": "^6",
    "babel-preset-node6": "^11",
    "babel-register": "^6",
    "eslint": "^3",
    "estraverse-fb": "^1",
    "gulp": "^3",
    "gulp-ava": "^0",
    "gulp-debug": "^2",
    "gulp-eslint": "^3",
    "gulp-if": "^2",
    "gulp-load-plugins": "^1",
    "gulp-nodemon": "^2",
    "gulp-sequence": "^0",
    "nyc": "^8",
    "supertest": "^2",
    "supertest-as-promised": "^4"
  }
}
